{
  "expo": {
    "name": "Fashion store",
    "slug": "myEX",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/store.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/store.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/store.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mohamed_ragab.myEX"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "16a87f11-c7fd-4101-9308-af9b7716859b"
      }
    }
  }
}
